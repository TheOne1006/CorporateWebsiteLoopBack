{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "globals": {
      "describe": true,
      "before": true,
      "after": true,
      "it": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
    },
    "plugins": [
    ],
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console":[
            "off"
        ]
    }
}
